<div class="blocks">

  <h2>your blocks</h2>

  <ul class="block-list">
    <% @blocks.each do |block| %>
      <li>@<%= block.screen_name %> | <%= block.name %> | <%= block.created_at %></li>
    <% end %>
  </ul>

  <h2>users</h2>
  <ul class="block-list">
    <% @users.each do |user| %>
      <li>@<%= user.twitter_screen_name %></li>
    <% end %>
  </ul>

</div>
